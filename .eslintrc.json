{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:react/recommended",
		"prettier",
		"prettier/react"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react", "import"],
	"rules": {
		"react/prop-types": 0,
		"no-console": 1
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
