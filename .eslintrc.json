{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
