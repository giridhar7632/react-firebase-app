{
	"name": "react-firebase-app",
	"version": "1.0.0",
	"description": "A starter template of React and Firebase",
	"main": "index.js",
	"scripts": {
		"dev": "parcel src/index.html",
		"build": "parcel src/index.html",
		"format": "prettier \"src/**/*.{js,html}\"  --write",
		"format:check": "prettier \"src/**/*.{js,html}\" --check",
		"lint": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
		"lint:check": "eslint \"src/**/*.{js,jsx}\" --quiet",
		"check": "npm run format:check && npm run lint:check",
		"check:fix": "npm run format && npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/giridhar7632/react-firebase-app.git"
	},
	"keywords": [
		"React",
		"ReactDOM",
		"JavaScript",
		"firebase",
		"starter-template",
		"starter-kit"
	],
	"author": "Talla Giridhar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/giridhar7632/react-firebase-app/issues"
	},
	"dependencies": {
		"firebase": "^8.2.4",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 2 Chrome versions",
			"last 2 ChromeAndroid versions",
			"last 2 Edge versions",
			"last 2 iOS versions",
			"last 2 Safari versions",
			"last 2 Firefox versions"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^4.3.8",
		"parcel-bundler": "^1.12.4",
		"prettier": "^2.2.1"
	}
}
